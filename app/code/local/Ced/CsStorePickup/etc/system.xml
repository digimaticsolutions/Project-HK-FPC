<config>
	<sections>		
		<ced_csmarketplace>
			<show_in_vendor>1</show_in_vendor>
			<groups>
				<csmultishipping>
					<fields>
						<storepickup_active translate="label">
							<label>Enable StorePickup Shipping For Vendor</label>
							<frontend_type>select</frontend_type>
							<frontend_model>csmultishipping/adminhtml_system_config_frontend_enable</frontend_model>
							<comment>Before Enabling this please enable StorePickup Shipping Method</comment>
							<config_path>ced_csstorepickup/general/active</config_path>
							<source_model>adminhtml/system_config_source_yesno</source_model>
							<sort_order>6</sort_order>
							<show_in_website>1</show_in_website>
							<show_in_default>1</show_in_default>
							<show_in_vendor>1</show_in_vendor>
						</storepickup_active> 
					</fields>   
				</csmultishipping>
			</groups>
		</ced_csmarketplace>
	</sections>
</config>